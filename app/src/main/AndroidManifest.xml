<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jhon.webservices">
    <uses-permission android:name="android.permission.INTERNET">

    </uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Webservices-Pet"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.jhon.webservices.menus.ActivityContacto"></activity>
        <activity android:name="com.jhon.webservices.menus.ActivityAbout" />
        <activity android:name="com.jhon.webservices.Mascotas"/>
        <activity android:name="com.jhon.webservices.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jhon.webservices.MisContactos"
            android:parentActivityName="com.jhon.webservices.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jhon.webservices.MainActivity" />
        </activity>
    </application>

</manifest>